{% for project in projects %}
    {% set path = path('pletnev_pm_admin_project' , {projectId : project.id}) %}
    {% set icon = project.icon is defined ? '<i class="glyphicon glyphicon-'~project.icon~'"></i>' : '' %}
    {% set active = app.request.get('projectId') == project.id ? 'active' : '' %}
    <li class="{{ active }}"><a class="" href="{{ path }}">{{ icon|raw }}<span> {{ project.title }}</span></a></li>
{% endfor %}